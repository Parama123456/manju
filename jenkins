pipeline() {
 agent any
   stages{
   stage('clean up')
        steps {
        sh '''
           docker rm k1
           docker rmi kar_image
        '''
        }
        }
        
     stage('build_image'){
       step{
          sh "docker build -t kar_image"
        }
}
stage('create container'){
 step{
  sh "docker run -it --name k1 -p 5000:80 kar_image /bin/bash"
}
}

}
}

