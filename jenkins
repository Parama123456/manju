pipeline() {
 agent any
   stages{
   stage('clean up') {
        steps {
        sh '''
           docker rm -f k1
           docker rmi kar_image
        '''
        }
        }
        
     stage('build_image'){
       steps{
          sh "docker build -t kar_image ."
        }
}
stage('create container'){
 steps{
  sh "docker run -d --name k1 -p 5000:80 kar_image /bin/bash"
}
}

}
}

